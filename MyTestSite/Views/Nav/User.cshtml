@model Dictionary<string, string>
@using MyTestSite.Models

@Html.ActionLink("Home", "Index", "Home", null, new { @class = "btn btn-block btn-default btn-lg" })


@foreach (var link in Model)
{
    @Html.RouteLink(link.Value, new
{
    controller = "Home",
    action = "Index",
    category = link.Key,
    page = 1
}, new
{
    @class = "btn btn-block btn-default btn-lg"
})
}


